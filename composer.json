{
    "name": "lumphp/laravel-validation",
    "description": "The Illuminate Validation package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-validation/issues",
        "source": "https://github.com/lumphp/laravel-validation"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "egulias/email-validator": "^2.1.10",
        "lumphp/laravel-collections": "^8.0",
        "lumphp/laravel-container": "^8.0",
        "lumphp/laravel-contracts": "^8.0",
        "lumphp/laravel-macroable": "^8.0",
        "lumphp/laravel-support": "^8.0",
        "lumphp/laravel-translation": "^8.0",
        "symfony/http-foundation": "^5.1.4",
        "symfony/mime": "^5.1.4"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Validation\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "suggest": {
        "lumphp/laravel-database": "Required to use the database presence verifier (^8.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
